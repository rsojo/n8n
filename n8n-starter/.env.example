# n8n environment example â€” copia este archivo a `.env` y rellena tus valores

# Core
DATA_FOLDER=./n8n-data
DOMAIN_NAME=example.com
SUBDOMAIN=n8n
N8N_BASIC_AUTH_USER=change-me
N8N_BASIC_AUTH_PASSWORD=change-me
GENERIC_TIMEZONE=Europe/Berlin

# Editor/CORS (ajusta si usas Codespaces o localhost)
N8N_CORS_ALLOW_ORIGIN=http://localhost:5678
N8N_EDITOR_BASE_URL=http://localhost:5678

# Proyecto
GCAL_CALENDAR_ID=primary
WABA_PHONE_NUMBER_ID=PHONE_NUMBER_ID
WABA_TOKEN=REEMPLAZAR_CON_TU_WABA_TOKEN

# Google Calendar API (no subas valores reales al repo)
GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALENDAR_REDIRECT_URI=your_google_redirect_uri

# n8n runtime
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
DB_SQLITE_POOL_SIZE=5
N8N_RUNNERS_ENABLED=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
